{
  "name": "@ringai/test-utils",
  "version": "0.8.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ringcentral/ringai.git"
  },
  "author": "RingCentral",
  "license": "MIT",
  "scripts": {
    "test": "echo 'Run tests from root: pnpm test:unit'",
    "test:watch": "cd ../.. && vitest watch packages/test-utils/test",
    "build": "tsup --config ../../tsup.config.base.ts && tsc -p tsconfig.build.json --declaration --emitDeclarationOnly",
    "build:watch": "tsc --watch --inlineSourceMap"
  },
  "dependencies": {
    "@ringai/types": "workspace:*"
  },
  "devDependencies": {
    "@types/chai": "5.0.1",
    "@types/mocha": "10.0.9",
    "@types/sinon": "17.0.4",
    "@types/sinon-chai": "3.2.12",
    "chai": "4.3.10",
    "sinon": "19.0.2",
    "sinon-chai": "3.7.0"
  }
}
