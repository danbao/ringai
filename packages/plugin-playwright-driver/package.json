{
  "name": "@ringai/plugin-playwright-driver",
  "version": "0.8.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./src/index.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ringcentral/ringai.git"
  },
  "author": "RingCentral",
  "license": "MIT",
  "scripts": {
    "test": "echo 'Run tests from root: pnpm test:unit'",
    "test:debug": "cd ../.. && PLAYWRIGHT_DEBUG=1 vitest run packages/plugin-playwright-driver/test",
    "postinstall": "node scripts/install-browsers.cjs",
    "install-browsers": "node scripts/install-browsers.cjs",
    "uninstall-browsers": "npx playwright uninstall --all",
    "build": "tsup && tsc -p tsconfig.build.json --declaration --emitDeclarationOnly"
  },
  "dependencies": {
    "@ringai/logger": "workspace:*",
    "@ringai/plugin-api": "workspace:*",
    "@ringai/timeout-config": "workspace:*",
    "@ringai/types": "workspace:*",
    "@types/node": "22.8.5",
    "playwright": "^1.58.0"
  },
  "devDependencies": {
  }
}
