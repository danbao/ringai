{
  "name": "@ringai/e2e-test-app",
  "private": true,
  "version": "0.8.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ringcentral/ringai.git"
  },
  "author": "RingCentral",
  "license": "MIT",
  "scripts": {
    "test": "pnpm run test:unit && pnpm run test:e2e",
    "test:all": "pnpm run test:unit && pnpm run test:e2e:full && pnpm run test:integration",

    "test:unit": "pnpm run test:simple",
    "test:simple": "ringai run --config test/simple/.ringairc --env-parameters.test 10 --rc.tags-list=#P0,#P1",
    "test:e2e": "pnpm run test:playwright:headless",
    "test:e2e:full": "pnpm run test:playwright:headless && pnpm run test:screenshots",
    "test:playwright": "tsx src/test-runner.ts --config ./test/playwright/config.cjs --env-config=./test/playwright/env.json",
    "test:playwright:headless": "tsx src/test-runner.ts --config ./test/playwright/config.cjs --env-config=./test/playwright/env.json --headless",
    "test:screenshots": "tsx src/test-runner.ts --config ./test/playwright/config-screenshot.cjs",
    "test:integration": "mocha test/integration/**/*.spec.cjs --timeout 120000",
    "test:integration:process": "mocha test/integration/process-*.spec.cjs --timeout 120000",
    "test:integration:config": "mocha test/integration/config-*.spec.cjs --timeout 120000",
    "test:integration:error": "mocha test/integration/error-*.spec.cjs --timeout 120000",
    "test:integration:performance": "mocha test/integration/performance-*.spec.cjs --timeout 120000",
    "dev:local": "pnpm run sync:html && tsx src/mock-web-server.ts",
    "dev:worker": "pnpm run sync:html && wrangler dev",
    "deploy:worker": "pnpm run sync:html && wrangler deploy",
    "start": "pnpm run dev:local",
    "deploy": "./scripts/deploy.sh",
    "sync:html": "node scripts/sync-html-references.cjs"
  },
  "dependencies": {
    "@hono/node-server": "^1.12.2",
    "@ringai/cli": "workspace:*",
    "@ringai/plugin-babel": "workspace:*",
    "@ringai/plugin-fs-store": "workspace:*",
    "@ringai/plugin-playwright-driver": "workspace:*",
    "@ringai/web-application": "workspace:*",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^10.0.0",
    "@babel/preset-env": "^7.26.0",
    "babel-preset-es2015": "6.24.1",
    "c8": "10.1.3",
    "chai": "^4.3.0",
    "concurrently": "9.0.1",
    "hono": "^4.6.3",
    "mocha": "^10.0.0",
    "ringai": "workspace:*",
    "ts-node": "10.9.2"
  },
  "devDependencies": {
    "wrangler": "4.26.0"
  }
}
