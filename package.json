{
  "name": "ringai-build",
  "private": true,
  "author": "RingCentral",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ringcentral/ringai.git"
  },
  "scripts": {
    "cleanup": "pnpm run cleanup:packages && pnpm run cleanup:root",
    "cleanup:root": "node ./utils/cleanup.js",
    "cleanup:packages": "pnpm -r exec -- node ../../utils/cleanup",
    "reinstall": "pnpm run cleanup && pnpm install && pnpm run build",
    "utils:add-package-files": "pnpm -r exec -- node ../../utils/add-package-files",
    "utils:generate-readme": "pnpm -r exec -- node ../../utils/generate-readme",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "pnpm run test:unit && pnpm run test:e2e:headless",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "pnpm --filter @ringai/e2e-test-app run test",
    "test:e2e:headless": "pnpm --filter @ringai/e2e-test-app run test:playwright:headless",
    "test:e2e:simple": "pnpm --filter @ringai/e2e-test-app run test:simple",
    "test:e2e:coverage": "c8 --config .c8.json tsx ./packages/e2e-test-app/src/test-runner.ts --config ./packages/e2e-test-app/test/playwright/config.coverage.cjs --env-config=./packages/e2e-test-app/test/playwright/env.json --headless",
    "test:performance": "pnpm --filter @ringai/fs-reader run test:performance",
    "test:playwright": "pnpm --filter @ringai/plugin-playwright-driver run test",
    "test:playwright:debug": "cd packages/plugin-playwright-driver && pnpm run test:debug",
    "test:ci": "pnpm run test:unit:coverage && pnpm run test:e2e:coverage",
    "test:ci:coverage": "pnpm run test:unit:coverage:lcov && pnpm run test:e2e:coverage:lcov",
    "test:unit:coverage:lcov": "vitest run --coverage --reporter=verbose",
    "test:e2e:coverage:lcov": "c8 tsx ./packages/e2e-test-app/src/test-runner.ts --config ./packages/e2e-test-app/test/playwright/config.coverage.cjs --env-config=./packages/e2e-test-app/test/playwright/env.json --headless",
    "test:e2e:grid": "echo 'Selenium Grid tests removed â€” use test:e2e:headless for Playwright E2E' && exit 0",
    "build": "turbo run build",
    "build:main": "turbo run build --filter=!@ringai/e2e-test-app --filter=!@ringai/devtool-frontend --filter=!@ringai/devtool-extension",
    "build:main:watch": "pnpm --filter=!@ringai/e2e-test-app --filter=!@ringai/devtool-frontend --filter=!@ringai/devtool-extension -r exec -- tsc --watch --inlineSourceMap",
    "build:devtool": "turbo run build --filter=@ringai/devtool-frontend",
    "build:extension": "turbo run build --filter=@ringai/devtool-extension",
    "build:types:check": "turbo run build --filter=!@ringai/e2e-test-app --filter=!@ringai/devtool-frontend --filter=!@ringai/devtool-extension",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "publish:version": "changeset version",
    "publish:ci": "node ./utils/publish.js --exclude=@ringai/devtool-frontend,@ringai/devtool-backend,@ringai/devtool-extension",
    "publish:dev": "node ./utils/publish.js --dev --exclude=@ringai/devtool-frontend,@ringai/devtool-backend,@ringai/devtool-extension",
    "deps:check": "pnpm run deps:check:precommit",
    "deps:check:precommit": "pnpm run deps:validate",
    "deps:validate": "pnpm -r exec -- node ../../utils/check-packages-versions",
    "deps:find-updates": "ncu -m --deep --color",
    "deps:update": "ncu -m --deep --color",
    "docs:api": "typedoc",
    "docs:api:watch": "typedoc --watch",
    "docs": "pnpm run docs:api",
    "analyze:performance:logs-only": "node scripts/analyze-long-steps.js"
  },
  "nyc": {
    "report-dir": "./.coverage",
    "all": false,
    "check-coverage": false,
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "branches": 45,
    "sourceMap": false,
    "include": [
      "core/*/src/*.ts",
      "core/*/src/**/*.ts",
      "packages/*/src/*.ts",
      "packages/*/src/**/*.ts"
    ],
    "exclude": [
      "core/*/src/index.ts",
      "core/types",
      "packages/*/src/index.ts",
      "packages/web-application/src/web-application.ts",
      "packages/web-application/src/web-client.ts",
      "packages/plugin-playwright-driver/**/*",
      "packages/devtool-*/**/*",
      "packages/e2e-test-app/**/*",
      "**/test/**/*",
      "**/tests/**/*",
      "**/*.spec.ts",
      "**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "cache": true,
    "temp-dir": "./.nyc_output"
  },
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "chromedriver",
      "core-js",
      "cpu-features",
      "edgedriver",
      "esbuild",
      "geckodriver",
      "nx",
      "pre-commit",
      "sharp",
      "spawn-sync",
      "ssh2",
      "workerd"
    ]
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.2",
    "@changesets/cli": "2.29.8",
    "@eslint/js": "^9.39.2",
    "@jsdevtools/npm-publish": "3.1.1",

    "@types/chai": "5.0.1",
    "@types/mocha": "10.0.9",
    "@types/node": "22.19.11",
    "@types/sinon": "17.0.4",
    "@vitest/coverage-v8": "4.0.18",
    "chai": "4.3.10",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-sonarjs": "4.0.0",
    "glob": "11.0.0",
    "globals": "17.3.0",

    "mocha": "10.8.2",
    "npm-check-updates": "17.1.9",
    "nyc": "17.1.0",
    "pre-commit": "1.2.2",
    "rimraf": "6.0.1",
    "sinon": "19.0.2",
    "ts-node": "10.9.2",
    "tsup": "8.5.1",
    "tsx": "4.21.0",
    "turbo": "2.8.10",
    "typedoc": "0.28.17",
    "typescript": "5.6.3",
    "typescript-eslint": "8.56.0",
    "vitest": "4.0.18"
  },
  "dependencies": {
    "@clack/prompts": "1.0.1",
    "citty": "^0.1.6",
    "hookable": "6.0.1",
    "kleur": "4.1.5",
    "tinyglobby": "0.2.15",
    "tinypool": "2.1.0"
  }
}
